doctype html
html(lang="zh-CN")
	head
		meta(charset="utf-8")
		meta(name="viewport" content="width=device-width, height=device-height, initial-scale=1.0, maximum-scale=1.0, user-scalable=0")
		meta(name="format-detection" content="telephone=no")
		title
		each lib in cssLibs
			link(rel="stylesheet" href=`${lib}`)
	body
		//this file is generated by /index.pug 
		app

		script.
			var _app_config = {
				ajax_prefix: "/reportapi/"
			};

		each lib in jsLibs
			script(src=`${lib}`)
		script(src=`${indexjs}`)

		script.
			function onPageChangeHook(pagename) {
				console.log(pagename);
			}
